{
    "name": "larabizcms/mediable",
    "description": "Media Management for Laravel",
    "keywords": [
        "PHP",
        "Laravel",
        "CMS",
        "File Management",
        "Media Management",
        "Media",
        "File"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "intervention/image": "^2.5",
        "spatie/image-optimizer": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "LarabizCMS\\Mediable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LarabizCMS\\Mediable\\Providers\\MediableServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LarabizCMS\\Mediable\\Tests\\": "tests/"
        }
    }
}
